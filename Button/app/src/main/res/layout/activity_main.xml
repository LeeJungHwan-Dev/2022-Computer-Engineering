<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Go_naver"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="네이버 가기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386" />

    <Button
        android:id="@+id/Go_phone"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="핸드폰 다이얼"
        app:layout_constraintEnd_toEndOf="@+id/Go_naver"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Go_naver"
        app:layout_constraintTop_toBottomOf="@+id/Go_naver" />

    <Button
        android:id="@+id/view_Toast"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="메시지 출력"
        app:layout_constraintEnd_toEndOf="@+id/Go_naver"
        app:layout_constraintStart_toStartOf="@+id/Go_naver"
        app:layout_constraintTop_toBottomOf="@+id/Go_phone" />
</androidx.constraintlayout.widget.ConstraintLayout>